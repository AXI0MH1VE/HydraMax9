# AXIOM HIVE HydraMax9 Configuration

# Application
NODE_ENV=development
VITE_APP_NAME="AXIOM HIVE HydraMax9"
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here
VITE_GEMINI_MODEL=gemini-pro
VITE_GEMINI_MAX_TOKENS=2048

# Security
VITE_ENABLE_SECURITY_MODULE=true
VITE_ENTROPY_THRESHOLD=0.75
VITE_MAX_ANOMALY_CONFIDENCE=0.85

# Performance Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_TELEMETRY_INTERVAL=1000
VITE_MAX_TELEMETRY_POINTS=100

# Feature Flags
VITE_ENABLE_AI_CHAT=true
VITE_ENABLE_NEURAL_GRAPH=true
VITE_ENABLE_TERMINAL=true
VITE_ENABLE_BOOTSTRAP_SEQUENCE=true

# Development
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# iOS App Configuration (for future mobile integration)
IOS_BUNDLE_ID=com.axiomhive.hydramax9
IOS_APP_NAME=HydraMax9
